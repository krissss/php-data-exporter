{
    "name": "kriss/data-export",
    "description": "Data Export For PHP",
    "keywords": [
        "kriss",
        "data-export"
    ],
    "homepage": "https://github.com/krissss/php-data-export",
    "license": "MIT",
    "authors": [
        {
            "name": "kriss",
            "email": "462679766@qq.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/container": ">=5",
        "illuminate/support": ">=5",
        "sonata-project/exporter": "^2.13"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "openspout/openspout": "^3.7",
        "pestphp/pest": "^1.20",
        "phpoffice/phpspreadsheet": "^1.21",
        "symfony/http-foundation": ">=3"
    },
    "suggest": {
        "phpoffice/phpspreadsheet": "export xlsx",
        "box/spout": "export xlsxSpout",
        "openspout/openspout": "box/spout replacer"
    },
    "autoload": {
        "psr-4": {
            "Kriss\\DataExporter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kriss\\DataExporter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
